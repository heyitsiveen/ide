    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "Convex": {
      "command": "npx -y convex@latest mcp start",
      "env": {},
      "args": []
    },
    "Figma": {
      "url": "https://mcp.figma.com/mcp",
      "headers": {}
    },
    "Neon": {
      "command": "npx -y mcp-remote https://mcp.neon.tech/mcp",
      "env": {},
      "args": []
    },
    "Shopify": {
      "command": "npx -y @shopify/dev-mcp@latest",
      "env": {},
      "args": []
    }
  }
